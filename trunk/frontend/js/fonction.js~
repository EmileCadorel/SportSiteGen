function verif_form_connexion(f){

    var rep = true;
    if(f.pseudo.value.length == 0){
	f.pseudo.style.backgroundColor = "#fba";
	rep = false;
    }
    else {
	f.pseudo.style.backgroundColor = "#63DB57";
    }
    if(f.password.value.length == 0){
	f.password.style.backgroundColor = "#fba";
	rep = false;
    }
    else 

}
